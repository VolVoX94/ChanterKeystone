<div class="container">
	{{#if honor}}
		{{#each honor}}
			<h2>Members</h2>
			{{#each members}}
				<button type="button" class="btn bg-transparent border-button" data-toggle="modal" data-target="#d{{@index}}">
					<p>{{name.first}} {{name.last}} ({{comiteRole}})</p>
				</button>
				<div class="modal fade" id="d{{@index}}" tabindex="-1" role="dialog" aria-labelledby="Modal1" aria-hidden="true">
					<div class="modal-dialog modal-dialog-centered modal-lg">
						<div class="modal-content">
							<div class="modal-header">
								<h5 class="modal-title" id="exampleModalLabel">{{name.first}} {{name.last}} ({{comiteRole}}</h5>
								<button type="button" class="close" data-dismiss="modal" aria-label="Close">
									<span aria-hidden="true">&times;</span>
								</button>
							</div>
							<div class="modal-body">
								{{#if image}}
									<img src="{{cloudinaryUrl image	 height='400'}}">
								{{/if}}
								<h4>Contact</h4>
								<div class="row">
									<div class="col-lg-3">
										<p><strong>Email: </strong>{{email}}</p>
									</div>
									<div class="col-lg-3">
										<p><strong>Place: </strong>{{place}}</p>
									</div>
									<div class="col-lg-3">
										<p><strong>Phone: </strong>{{businessPhone}}</p>
									</div>
									<div class="col-lg-3">
										<p><strong>Fax: </strong>{{fax}}</p>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			{{/each}}
		{{/each}}
	{{/if}}
</div>
