	<div class="tab-content">
		<div id="home" class="tab-pane fade in active">

			<form action="/dashboardUser" method="post" class="form-horizontal">
				<div id="heada" class="">
					<h1>
						{{#if isGerman}}
							Mein Benutzer:
						{{else}}
							Mon utilisateur:
						{{/if}}
					</h1>
				</div>
				<input type="hidden" class="form-control" name="id" value="{{user._id}}">

				<div class="form-group">
					<label class="control-label col-sm-2" for="email">
						{{#if isGerman}}
							Vorname:
						{{else}}
							Prénom:
						{{/if}}
					</label>
					<div class="col-sm-10">
						<input required type="text" value="{{user.name.first}}" class="form-control" name="firstname"
							   placeholder="Enter firstname">
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-2" for="email">
						{{#if isGerman}}
							Nachname:
						{{else}}
							Nom de famille:
						{{/if}}
					</label>
					<div class="col-sm-10">
						<input required type="text" name="lastname" value="{{user.name.last}}" class="form-control"
							   placeholder="Enter lastname">
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-2" for="email">Email:</label>
					<div class="col-sm-10">
						<input required type="email" name="email" value="{{user.email}}" class="form-control"
							   placeholder="Enter email">
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-2" for="address">Adresse:</label>
					<div class="col-sm-10">
						<input required type="text" name="address" value="{{user.address}}" class="form-control"
							   placeholder="Enter place">
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-2" for="address">
						{{#if isGerman}}
							Zweite Adresse:
						{{else}}
							Deuxième adresse:
						{{/if}}
					</label>
					<div class="col-sm-10">
						<input type="text" name="secondAddress" value="{{user.secondAddress}}" class="form-control"
							   placeholder="Enter place">
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-2" for="plz">
						{{#if isGerman}}
							PLZ:
						{{else}}
							Postcode:
						{{/if}}
					</label>
					<div class="col-sm-10">
						<input required type="number" name="plz" value="{{user.plz}}" class="form-control"
							   placeholder="Enter plz">
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-2" for="place">
						{{#if isGerman}}
							Ort:
						{{else}}
							lieu:
						{{/if}}
					</label>
					<div class="col-sm-10">
						<input required type="text" name="place" value="{{user.place}}" class="form-control"
							   placeholder="Enter place">
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-2" for="privatePhone">
						{{#if isGerman}}
							Telefon privat:
						{{else}}
							Téléphone privé:
						{{/if}}
					</label>
					<div class="col-sm-10">
						<input type="text" name="privatePhone" value="{{user.privatePhone}}" class="form-control"
							   placeholder="Enter private phone">
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-2" for="businessPhone">
						{{#if isGerman}}
							Telefon geschäftlich:
						{{else}}
							Téléphone professionnel:
						{{/if}}
					</label>
					<div class="col-sm-10">
						<input type="text" name="businessPhone" value="{{user.businessPhone}}" class="form-control"
							   placeholder="Enter business phone">
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-2" for="fax">
						Fax:
					</label>
					<div class="col-sm-10">
						<input type="text" name="fax" value="{{user.fax}}" class="form-control" placeholder="Enter fax">
					</div>
				</div>


				<div class="form-group">
					<label class="control-label col-sm-2" for="email">
						{{#if isGerman}}
							Passwort:
						{{else}}
							mot de passe:
						{{/if}}
					</label>
					<div class="col-sm-10">
						<input required type="password" name="password" id="password" value="" class="form-control"
							   placeholder="Enter new password" onkeyup='check();'>
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-2" for="email">
						{{#if isGerman}}
							Passwort bestätigen:
						{{else}}
							Confirmer le mot de passe:
						{{/if}}
					</label>
					<div class="col-sm-10">
						<input required type="password" id="confirm_password" value="" class="form-control"
							   placeholder="Confirm new password" onkeyup='check();'>
					</div>
				</div>

				<div class="form-group">
					<label class="control-label col-sm-1" for="lastUpdate">
						{{#if isGerman}}
							Letztes update:
						{{else}}
							dernière modification:
						{{/if}}
					</label>
					<div class="col-sm-12">
						<input type="text" value="{{user.lastUpdate}}" class="form-control" name="lastUpdate" readonly>
					</div>
				</div>

				<div class="col-lg-10 col-lg-offset-2">
					<span id='message'></span>
				</div>

				<div class="form-group">
					<div class="col-sm-offset-2 col-sm-10">
						<div class="checkbox">
							<label><input name="subscriber"
										  type="checkbox"
								{{#if user.isSubscriber}}
										  checked
								{{/if}}
							>
								{{#if isGerman}}
									Newsletter abonniert
								{{else}}
									S'abonner à la newsletter
								{{/if}}
							</label>
						</div>
					</div>
				</div>

				<input type="hidden" name="admin" value="{{user.isAdmin}}">

				<div class="form-group">
					<div class="col-sm-offset-2 col-sm-10">
						<button id="submit" type="submit" value="submit" class="btn btn-default">Submit</button>
					</div>
				</div>
			</form>


		</div>
		<div id="menu1" class="tab-pane fade">


			{{#each choirs}}
				<div class="col-lg-12">
					<form action="/dashboardChoir" method="post" class="form-horizontal">
						<h1>
							{{#if ../isGerman}}
