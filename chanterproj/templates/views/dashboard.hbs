								{{/if}}
							</label>
							<div class="col-sm-10">
								<select name="cashier" class="form-control">
									<option value="{{cashier._id}}">{{cashier.name.first}} {{cashier.name.last}}</option>
									{{#each ../useritem}}
										<option value="{{_id}}">{{name.first}} {{name.last}}</option>
									{{/each}}
								</select>
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="secretary">
								{{#if ../isGerman}}
									Sekretär/in:
								{{else}}
									secrétaire:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<select name="secretary" class="form-control">
									<option value="{{secretary._id}}">{{secretary.name.first}} {{secretary.name.last}}</option>
									{{#each ../useritem}}
										<option value="{{_id}}">{{name.first}} {{name.last}}</option>
									{{/each}}
								</select>
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="foundingYear">
								{{#if ../isGerman}}
									Gründungsj.:
								{{else}}
									Année de fondation:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<input type="number" value="{{foundingYear}}" class="form-control" name="foundingYear"
									   placeholder="Enter founding year">
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="homepage">Homepage:</label>
							<div class="col-sm-10">
								<input type="text" value="{{homepage}}" class="form-control" name="homepage"
									   placeholder="Enter homepage">
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="typeChoir">
								{{#if ../isGerman}}
									Typ Chor:
								{{else}}
									Type chœurs:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<select name="typeChoir" class="form-control">
									<option value="{{typeChoir}}">{{typeChoir}}</option>
									<option value="CX">CX</option>
									<option value="CD">CD</option>
									<option value="CJ">CJ</option>
									<option value="CH">CH</option>
									<option value="CE">CE</option>
								</select>
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="secondTypeChoir">
								{{#if ../isGerman}}
									Zweiter Chor Typ:
								{{else}}
									Deuxième type chœurs:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<select name="secondTypeChoir" class="form-control">
									<option value="{{secondTypeChoir}}">{{secondTypeChoir}}</option>
									<option value="CX">CX</option>
									<option value="CD">CD</option>
									<option value="CJ">CJ</option>
									<option value="CH">CH</option>
									<option value="CE">CE</option>
								</select>
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="groupMt">
								{{#if ../isGerman}}
									GruppeMT:
								{{else}}
									GroupMT:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<select name="groupMt" class="form-control">
									<option value="{{groupMt}}">{{groupMt}}</option>
									<option value="BV_Y">BV_Y</option>
									<option value="BV_N">BV_N</option>
									<option value="UCC_Y">UCC_Y</option>
									<option value="UCC_N">UCC_N</option>
									<option value="VC_Y">VC_Y</option>
									<option value="VC_N">VC_N</option>
									<option value="VOG_Y">VOG_Y</option>
									<option value="VOG_N">VOG_N</option>
								</select>
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="entranceGroupMt">
								{{#if ../isGerman}}
									Eintritt Gruppe MT:
								{{else}}
									GroupeMT d'admission:
								{{/if}}
							</label>
							<div class="col-sm-10">
								{{entranceGroupMt}}
								<input type="date" value="" class="form-control" name="entranceGroupMt"
									   placeholder="Enter entrance GroupMt">
								<input type="hidden" value="{{entranceGroupMt}}" class="form-control"
									   name="entranceGroupMtPrevious">
							</div>
						</div>

						<div class="form-group">
							<div class="checkbox">
								<label class="control-label col-sm-2" for="chGospel"><b>ChGospel</b></label>
								<div class="col-sm-10">
									<input name="chGospel" type="checkbox" {{#if chGospel}}checked{{/if}}>
								</div>
							</div>
						</div>

						<div class="form-group">
							<div class="checkbox">
								<label class="control-label col-sm-2" for="chGospel"><b>ChEglise</b></label>
								<div class="col-sm-10">
									<input name="chEglise" type="checkbox" {{#if chEglise}}checked{{/if}}>
								</div>
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="type2">
								{{#if ../isGerman}}
									Typ 2:
								{{else}}
									Type2:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<input type="text" value="{{type2}}" class="form-control" name="type2"
									   placeholder="Enter type2">
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="memberUSC">
								{{#if ../isGerman}}
									Mitglieder USC:
								{{else}}
									Membres USC:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<input type="text" value="{{memberUSC}}" class="form-control" name="memberUSC"
									   placeholder="Enter memberUSC">
							</div>
						</div>


						<div class="form-group">
							<label class="control-label col-sm-2" for="entranceUSC">
								{{#if ../isGerman}}
									Eintritt USC:
								{{else}}
									Inscription USC:
								{{/if}}
							</label>
							<div class="col-sm-10">
								{{entranceUSC}}
								<input type="date" value="{{entranceUSC}}" class="form-control" name="entranceUSC"
									   placeholder="Enter entrance entranceUSC">
								<input type="hidden" value="{{entranceUSC}}" class="form-control"
									   name="entranceUSCprevious">
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="memberFC">
								{{#if ../isGerman}}
									Mitglieder FC:
								{{else}}
									Membres FC:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<input type="text" value="{{memberFC}}" class="form-control" name="memberFC"
									   placeholder="Enter memberFC">
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="entranceFC">
								{{#if ../isGerman}}
									Eintritt FC:
								{{else}}
									Inscription FC:
								{{/if}}
							</label>
							<div class="col-sm-10">
								{{entranceFC}}
								<input type="date" value="" class="form-control" name="entranceFC"
									   placeholder="Enter entrance entranceFC">
								<input type="hidden" value="{{entranceFC}}" class="form-control"
									   name="entranceFCprevious">
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="courierName">
								{{#if ../isGerman}}
									Kuriername:
								{{else}}
									nom de coursier:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<input type="text" value="{{courierName}}" class="form-control" name="courierName"
									   placeholder="Enter courierName">
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="dekanat">Dekanat:</label>
							<div class="col-sm-10">
								<input type="text" value="{{dekanat}}" class="form-control" name="dekanat"
									   placeholder="Enter dekanat">
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="lang">
								{{#if ../isGerman}}
									Sprache:
								{{else}}
									Langue:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<select name="lang" class="form-control">
									<option value="{{lang}}">{{lang}}</option>
									<option value="Deutsch">Deutsch</option>
									<option value="Français">Français</option>
									<option value="English">English</option>
									<option value="Italiano">Italiano</option>
									<option value="Español">Español</option>
								</select>
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-2" for="dekanat">
								{{#if ../isGerman}}
									Bemerk.:
								{{else}}
									Remarques:
								{{/if}}
							</label>
							<div class="col-sm-10">
								<input type="text" value="{{remarks}}" class="form-control" name="remarks"
									   placeholder="Enter remark">
							</div>
						</div>

						<div class="form-group">
							<label class="control-label col-sm-1" for="lastUpdate">
								{{#if ../isGerman}}
									Letztes update:
								{{else}}
									dernière modification:
								{{/if}}
							</label>
							<div class="col-sm-12">
								<input type="text" value="{{lastUpdate}}" class="form-control" name="lastUpdate"
									   readonly>
							</div>
						</div>

						<button id="submit1" type="submit" value="Submit" class="btn btn-default"
								onclick="setChoirsActive()">
							{{#if ../isGerman}}
								Speichern
							{{else}}
								confirmer
							{{/if}}
						</button>
					</form>

					<hr/>
				</div>

			{{/each}}


		</div>
